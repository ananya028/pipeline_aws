{
  "name": "mantine-vite-template",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode production",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --fix src",
    "prettier": "prettier --check \"**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"**/*.{ts,tsx}\"",
    "jest": "jest",
    "jest:watch": "jest --watch",
    "test": "pnpm run jest --passWithNoTests",
    "prepare": "husky install .husky",
    "pre-commit-lint": "pnpm lint-staged",
    "pre-push-lint": "rm -rf dist/"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@hotjar/browser": "^1.0.9",
    "@mantine/core": "6.0.0",
    "@mantine/dropzone": "^6.0.15",
    "@mantine/form": "6.0.0",
    "@mantine/hooks": "^6.0.13",
    "@mantine/notifications": "^6.0.14",
    "@tabler/icons-react": "^2.22.0",
    "axios": "^1.4.0",
    "eventemitter3": "^5.0.1",
    "jest-transform-stub": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.12.1",
    "react-zoom-pan-pinch": "^3.1.0",
    "vite-plugin-environment": "^1.1.3",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.4.0",
    "@types/node": "^20.3.1",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-mantine": "2.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.3",
    "terser": "^5.18.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-sitemap": "^0.4.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npx prettier --write",
      "eslint src/*.{js,jsx,ts,tsx} --format unix --fix-dry-run --no-error-on-unmatched-pattern",
      "pnpm run test"
    ]
  }
}
